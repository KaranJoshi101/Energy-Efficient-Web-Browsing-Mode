{
    "manifest_version": 3,
    "name": "Energy Efficient Web Browsing",
    "version": "1.0",
    "description": "Optimize browsing to save energy by managing tabs, blocking media, and enabling dark mode.",
    "icons": {
      "1": "icons/icon.png"
    },
    "permissions": [
      "tabs", "storage", "background", "activeTab", "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png",
      "default_title": "Energy Efficient Browsing"
    },
    "background": {
      "service_worker": "background.js"
    }
}